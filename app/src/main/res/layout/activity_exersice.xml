        app:layout_constraintEnd_toEndOf="@+id/tv_ex_activity_title_exName"
        app:layout_constraintStart_toStartOf="@+id/tv_ex_activity_title_exName"
        app:layout_constraintTop_toTopOf="@+id/tv_ex_activity_title_exName" />

    <ImageView
        android:id="@+id/iv_ex_activity_title_nextGreen"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_ex_activity_title_exNumber"
        app:layout_constraintEnd_toStartOf="@+id/guideline30"
        app:layout_constraintStart_toStartOf="@+id/guideline30"
        app:layout_constraintTop_toTopOf="@+id/guideline23"
        app:srcCompat="@drawable/next" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <ImageView
        android:id="@+id/iv_ex_activity_title_backGreen"
        android:layout_width="wrap_content"
        android:layout_height="0dp"

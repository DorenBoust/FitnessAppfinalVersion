            app:layout_constraintEnd_toEndOf="@+id/tv_ex_activity_title_day"
            app:layout_constraintStart_toStartOf="@+id/tv_ex_activity_title_day"
            app:layout_constraintTop_toTopOf="@+id/guideline28" />

        <ImageView
            android:id="@+id/stop_workout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_ex_activity_title_day"
            app:layout_constraintEnd_toStartOf="@+id/tv_ex_activity_title_day"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_ex_activity_title_day"
            app:srcCompat="@drawable/ic_pause_24dp" />

        <TextView
            android:id="@+id/stop_workout_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/open_sans_hebrew_light"
            android:text="עצור אימון"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/stop_workout"
            app:layout_constraintStart_toStartOf="@+id/stop_workout"
            app:layout_constraintTop_toBottomOf="@+id/stop_workout" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/tv_ex_activity_title_exName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/open_sans_hebrew_bold"
        android:inAnimation="@anim/faidin"
        android:outAnimation="@anim/faidout"
        android:text="יד קידמית"
        android:textColor="@color/waite"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline30"
        app:layout_constraintStart_toStartOf="@+id/guideline32"
        app:layout_constraintTop_toTopOf="@+id/guideline23" />

    <TextView
        android:id="@+id/tv_ex_activity_title_exName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/open_sans_hebrew_bold"
        android:text="מלא נתונים"
        android:textColor="@color/waite"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline30"
        app:layout_constraintStart_toStartOf="@+id/guideline32"
        app:layout_constraintTop_toBottomOf="@+id/ex_activity_details" />

    <TextView
        android:id="@+id/tv_ex_activity_title_exNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
